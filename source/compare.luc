module compare (
    input z,  
    input v,  
    input n,
    input alufnc[6],
    output out[16]
  ) {
 
  always {
     
      out = 16b0;
      case (alufnc[2:1]){
       
      b01:
        out[0] = z;
      
      b10:
        out[0] = n ^ v;
      
      b11:
        out[0] = z | (n ^ v);
      
      default:
        out[0] = 0;  
  }
}
